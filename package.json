{
  "name": "dkcutter-nextjs",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.7.1",
  "description": "DKCutter NextJs is a framework for quickly starting Next.Js projects.",
  "author": "DKSHS",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@clerk/nextjs": "^5.3.1",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dkshs/eslint-config": "2.9.2",
    "@t3-oss/env-nextjs": "^0.11.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.15",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.3",
    "colorette": "^2.0.20",
    "dkcutter": "4.1.2",
    "eslint": "^8.57.0",
    "execa": "^9.3.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.17.0",
    "type-fest": "^4.24.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
