{
  "name": "dkcutter-nextjs",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.6.1",
  "description": "DKCutter NextJs is a framework for quickly starting Next.Js projects.",
  "author": "DKSHS",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@clerk/nextjs": "^6.12.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@dkshs/eslint-config": "4.3.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.10",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.0.10",
    "colorette": "^2.0.20",
    "dkcutter": "4.5.0",
    "eslint": "^9.21.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "next": "15.2.1",
    "next-auth": "^4.24.11",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "type-fest": "^4.37.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "zod": "^3.24.2"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
