{
  "name": "dkcutter-nextjs",
  "version": "1.0.0",
  "packageManager": "pnpm@9.0.6",
  "description": "DKCutter NextJs is a framework for quickly starting Next.Js projects.",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint . && prettier . -c",
    "lint:fix": "eslint --fix . && prettier . -w",
    "check": "pnpm lint && pnpm typecheck",
    "test": "tsx tests/index.ts",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "author": "DKSHS",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@clerk/nextjs": "^5.0.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dkshs/eslint-config": "1.5.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.7",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.2.79",
    "colorette": "^2.0.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "ora": "^8.0.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "tsx": "^4.7.2",
    "type-fest": "^4.17.0",
    "typescript": "^5.4.5",
    "zod": "^3.23.4"
  },
  "dependencies": {
    "dkcutter": "4.0.3"
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": [
      "pnpm lint:fix"
    ]
  }
}
