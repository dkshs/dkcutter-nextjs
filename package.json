{
  "name": "dkcutter-nextjs",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.14.4",
  "description": "DKCutter NextJs is a framework for quickly starting Next.Js projects.",
  "author": "DKSHS",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@clerk/nextjs": "^6.5.2",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@dkshs/eslint-config": "3.3.1",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.12",
    "colorette": "^2.0.20",
    "dkcutter": "4.1.4",
    "eslint": "^9.16.0",
    "execa": "^9.5.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "next": "15.0.3",
    "next-auth": "^4.24.10",
    "ora": "^8.1.1",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "type-fest": "^4.29.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
