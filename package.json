{
  "name": "dkcutter-nextjs",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.1.4",
  "description": "DKCutter NextJs is a framework for quickly starting Next.Js projects.",
  "author": "DKSHS",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@clerk/nextjs": "^5.1.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dkshs/eslint-config": "2.5.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.1",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.3",
    "colorette": "^2.0.20",
    "dkcutter": "4.1.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "execa": "^9.1.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.11.2",
    "type-fest": "^4.18.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  }
}
